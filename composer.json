{
    "name": "dominikzogg/contao-newspagination",
    "description": "Adds a pagination to the newsreader module",
    "keywords": ["contao", "news", "pagination"],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "contao/core-bundle": "^3.5 || ^4.4",
        "contao-community-alliance/composer-plugin": "^2.4 || ^3.0"
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/newspagination"
            }
        }
    },
    "replace": {
        "contao-legacy/newspagination": "self.version"
    }
}
